      <char id="0x48" print="?"      >a.......i....n</char>  <!-- HAN hangman with left leg -->
      <char id="0x49" print="?"      >a.......i...mn</char>  <!-- HAN hangman with left, right legs -->
      <char id="0x4a" print="?"      >a.....g.i...mn</char>  <!-- HAN hangman with both legs, left arm -->
      <char id="0x4b" print="?"      >a.....ghi...mn</char>  <!-- HAN full hangman -->
