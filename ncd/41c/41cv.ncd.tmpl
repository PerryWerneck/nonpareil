      <char id="0x7a" print="z"      >...d..g......n</char>
      <char id="0x7b" print="{"      >.......h..k.m.</char>  <!-- LEF left brace -->
      <char id="0x7c" print="?"      >...d......k.mn</char>  <!-- DEL delta -->
