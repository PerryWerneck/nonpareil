      <char id="0x43" print="c"      >...de.gh......</char>
      <char id="0x44" print="d"      >.bcde.gh......</char>
      <char id="0x45" print="e"      >...de.g.......</char>
      <char id="0x46" print="?"      >a.............</char>  <!-- OVE overbar (hangman character - head only) -->
