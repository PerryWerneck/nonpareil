      <char id="0x2c" print="?"      >......gh..k.m.</char>  <!-- left goose -->
      <char id="0x2d" print="-"      >......gh......</char>
      <char id="0x2e" print="?"      >......gh...l.n</char>  <!-- right goose -->
      <char id="0x2f" print="/"      >..........k..n</char>
      <char id="0x30" print="0"      >abcdef....k..n</char>  <!-- zero with slash to distinguish from letter O -->
