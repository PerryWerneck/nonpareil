      <char id="0x5e" print="?"      >...........lmn</char>  <!-- DC3 Greek lamda -->
      <char id="0x5f" print="?"      >...d.....jk..n</char>  <!-- ANG angle symbol (duplicate of 0x4f) -->
      <char id="0x60" print="?"      >a.......i.....</char>  <!-- SUP high-T (hangman head and neck/torso) (duplicate of 0x47) -->
