      <char id="0x1b" print="["      >a..def........</char>
      <char id="0x1c" print="\"      >...........lm.</char>
      <char id="0x1d" print="]"      >abcd..........</char>
      <char id="0x1e" print="?"      >ab........k..n</char>
      <char id="0x1f" print="_"      >...d..........</char>
      <char id="0x20" print=" "      >..............</char>
