<!DOCTYPE calcdef SYSTEM "http://nonpareil.brouhaha.com/dtd/calcdef.dtd">

<calcdef version="1.0"
         arch="Woodstock"
         platform="Spice"
         model="34c"
         copyright="Copyright 2006 Eric Smith &lt;eric@brouhaha.com&gt;"
         license="CC-BY-SA-2.5">

  <keyboard>

    <switch number="0">
      <switch_pos position="0" flag="power" value="0"/>
      <switch_pos position="1" flag="power" value="1"/>
    </switch>

    <key user_keycode="11" hw_keycode="0064"/>
    <key user_keycode="12" hw_keycode="0063"/>
    <key user_keycode="13" hw_keycode="0062"/>
    <key user_keycode="14" hw_keycode="0061"/>
    <key user_keycode="15" hw_keycode="0060"/>

    <key user_keycode="21" hw_keycode="0224"/>
    <key user_keycode="22" hw_keycode="0223"/>
    <key user_keycode="23" hw_keycode="0222"/>
    <key user_keycode="24" hw_keycode="0221"/>
    <key user_keycode="25" hw_keycode="0220"/>

    <key user_keycode="31" hw_keycode="0163"/>
    <key user_keycode="32" hw_keycode="0162"/>
    <key user_keycode="33" hw_keycode="0161"/>
    <key user_keycode="34" hw_keycode="0160"/>

    <key user_keycode="41" hw_keycode="0243"/>
    <key user_keycode="42" hw_keycode="0242"/>
    <key user_keycode="43" hw_keycode="0241"/>
    <key user_keycode="44" hw_keycode="0240"/>

    <key user_keycode="51" hw_keycode="0143"/>
    <key user_keycode="52" hw_keycode="0142"/>
    <key user_keycode="53" hw_keycode="0141"/>
    <key user_keycode="54" hw_keycode="0140"/>

    <key user_keycode="61" hw_keycode="0323"/>
    <key user_keycode="62" hw_keycode="0322"/>
    <key user_keycode="63" hw_keycode="0321"/>
    <key user_keycode="64" hw_keycode="0320"/>

    <key user_keycode="71" hw_keycode="0103"/>
    <key user_keycode="72" hw_keycode="0102"/>
    <key user_keycode="73" hw_keycode="0101"/>
    <key user_keycode="74" hw_keycode="0100"/>
  </keyboard>

  <chip name="cpu">
    <part_info>
      <vendor_name>HP</vendor_name>
      <part_number>1820-2162-A</part_number>
    </part_info>
    <part_info>
      <vendor_name>AMI</vendor_name>
    </part_info>
    <inst_clock osc_type="LC" freq="140000"/>
    <memory addr_space="inst" banks="01" base_addr="00000" size="02000">
    </memory>
    <chargen>
      <char id="0x00" segments="abcdef  " />
      <char id="0x01" segments=" bc     " />
      <char id="0x02" segments="ab de g " />
      <char id="0x03" segments="abcd  g " />
      <char id="0x04" segments=" bc  fg " />
      <char id="0x05" segments="a cd fg " />
      <char id="0x06" segments="a cdefg " />
      <char id="0x07" segments="abc     " />
      <char id="0x08" segments="abcdefg " />
      <char id="0x09" segments="abcd fg " />
      <char id="0x0a" segments="    e g " />  <!-- r -->
      <char id="0x0b" segments=" bc efg " />  <!-- H -->
      <char id="0x0c" segments="  cde g " />  <!-- o -->
      <char id="0x0d" segments="ab  efg " />  <!-- P -->
      <char id="0x0e" segments="a  defg " />  <!-- E -->
      <char id="0x0f" segments="        " />  <!-- space -->
      <char id="' '"    segments="        " />
      <char id="'-'"    segments="      g " />
      <char id="'.'"    segments="       h" />
    </chargen>
  </chip>

  <chip name="ROM/RAM">
    <memory addr_space="inst" banks="0" base_addr="02000" size="06000">
    </memory>
    <memory addr_space="inst" banks="1" base_addr="02000" size="06000">
    </memory>
    <memory addr_space="data" base_addr="000" size="0100">
    </memory>
  </chip>

  <obj_file addr_space="inst"
            copyright="Copyright 2004 Eric Smith &lt;eric@brouhaha.com&gt;"
            license="CC-BY-SA-2.5">
    34c.rom
  </obj_file>

