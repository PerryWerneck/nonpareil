global
	title "HP-80 Business Calculator"
	author "Eric Smith <eric@brouhaha.com>"
	hardware "Classic"
	model "80"
	rom "hp80.lst"
	image "hp80.jpg"
end

background
	offset 0 0
	size 303 551
end

display
	digits 15
	offset 45 53
	size 192 11
	color 0 0 0 0      # background
	color 1 0 0 0      # unlit segments
	color 2 255 17 17  # lit segments
end

